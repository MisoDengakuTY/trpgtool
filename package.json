{
  "name": "trpgtool",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@auth0/auth0-react": "^1.0.0",
    "@material-ui/core": "^4.10.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.0.0",
    "@types/nedb": "^1.8.10",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@urql/exchange-graphcache": "^3.0.1",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^2.15.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-reload": "^1.5.0",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.10.3",
    "mongoose": "^5.9.27",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-hooks": "^1.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "rsuite": "^4.7.2",
    "semantic-ui-react": "^0.88.2",
    "typescript": "~3.7.2",
    "yarn": "^1.22.4"
  },
  "homepage": "http://./",
  "main": "build/electron/main.js",
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"cd backend && nodemon server.js\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:windev": "concurrently \"cross-env BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.4",
    "@graphql-codegen/introspection": "1.15.4",
    "@graphql-codegen/typescript": "1.15.4",
    "@graphql-codegen/typescript-operations": "1.15.4",
    "@graphql-codegen/typescript-react-apollo": "1.15.4",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/mongoose": "^5.7.34",
    "chart.js": "^2.9.3",
    "concurrently": "^5.2.0",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "express-graphql": "^0.9.0",
    "nedb": "^1.8.0",
    "wait-on": "^5.0.1"
  },
  "description": "wara",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MisoDengakuTY/trpgtool.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MisoDengakuTY/trpgtool/issues"
  }
}
